<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="10" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="group1.LoginController">
    <children>
        <Label text="Username:" />
        <TextField fx:id="usernameField">
         <VBox.margin>
            <Insets left="150.0" right="150.0" />
         </VBox.margin></TextField>
        
        <Label text="Password:" />
        <PasswordField fx:id="passwordField">
         <VBox.margin>
            <Insets left="150.0" right="150.0" />
         </VBox.margin></PasswordField>
        
        <Button fx:id="loginButton" text="Login" />
    </children>
    <VBox.margin>
        <Insets bottom="20" left="20" right="20" top="20" />
    </VBox.margin>
</VBox>
